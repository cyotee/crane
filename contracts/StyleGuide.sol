    /* -------------------------------------------------------------------------- */

    /* -------------------------------------------------------------------------- */
    /*                               State Variable                               */
    /* -------------------------------------------------------------------------- */

    string internal _value;

    /* -------------------------------------------------------------------------- */
    /*                                   Events                                   */
    /* -------------------------------------------------------------------------- */

    /* -------------------------------------------------------------------------- */
    /*                                   Errors                                   */
    /* -------------------------------------------------------------------------- */

    /* -------------------------------------------------------------------------- */
    /*                                  Modifiers                                 */
    /* -------------------------------------------------------------------------- */

    /* -------------------------------------------------------------------------- */
