    /* -------------------------------------------------------------------------- */

    /* -------------------------------------------------------------------------- */
    /*                                 Constructor                                */
    /* -------------------------------------------------------------------------- */

    constructor() {}

    /* -------------------------------------------------------------------------- */
    /*                                   Receive                                  */
    /* -------------------------------------------------------------------------- */

    receive() external payable {}

    /* -------------------------------------------------------------------------- */
    /*                                  Fallback                                  */
    /* -------------------------------------------------------------------------- */

    fallback() external payable {}

    /* -------------------------------------------------------------------------- */
    /*                             External Functions                             */
    /* -------------------------------------------------------------------------- */

    /* ------------------------ State Changing Functions ------------------------ */

    function exampleExternalFunction(string calldata value_) external {
        _value = value_;
    }

    /* ----------------------------- View Functions ----------------------------- */

    function exampleExternalViewFunction() external view returns (string memory) {
        return _value;
    }

    /* ------------------------------ Pure Function ----------------------------- */

    function exampleExternalPureFunction(uint256 a_, uint256 b_) external pure returns (uint256) {
        return a_ + b_;
    }

    /* -------------------------------------------------------------------------- */
    /*                              Public Functions                              */
    /* -------------------------------------------------------------------------- */

    /* ------------------------ State Changing Functions ------------------------ */

    function examplePublicFunction(string calldata value_) public {
        _value = value_;
    }

    /* ----------------------------- View Functions ----------------------------- */

    function examplePublicViewFunction() public view returns (string memory) {
        return _value;
    }

    /* ----------------------------- Pure Functions ----------------------------- */

    function examplePublicPureFunction(uint256 a_, uint256 b_) public pure returns (uint256) {
        return a_ * b_;
    }

    /* -------------------------------------------------------------------------- */
    /*                             Internal Functions                             */
    /* -------------------------------------------------------------------------- */

    /* ------------------------ State Changing Functions ------------------------ */

    function _exampleInternalFunction(string calldata value_) internal {
        _value = value_;
    }

    /* ----------------------------- View Functions ----------------------------- */

    /* -------------------------------------------------------------------------- */
    /*                              Private Functions                             */
    /* -------------------------------------------------------------------------- */

    /* ------------------------ State Changing Functions ------------------------ */

    /* ----------------------------- View Functions ----------------------------- */

    function _examplePrivateFunction() private view returns (string memory) {
        return _value;
    }

    /* ----------------------------- Pure Functions ----------------------------- */
